        <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="CreateUserIds" enabled="true">
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="script">
// creating unique user ids
number_of_users = vars.get(&quot;number_of_users&quot;).toInteger()
for(int index = 1; index&lt;(number_of_users+1); index ++) {
	// generate uuid as unique id
	def user_id = UUID.randomUUID().toString();

	// store the id in a property to share between different thread groups
	props.put(&quot;user_id_&quot; + index.toString(), user_id);


	def log_record = String.format(&quot;User id #%d, %s&quot;, index, user_id);
	log.info(log_record);	
}</stringProp>
          <stringProp name="scriptLanguage">groovy</stringProp>
        </JSR223Sampler>
        <hashTree/>
